<?xml version="1.0"?>
<configuration smlns="http://schemas.microsoft.com/.NetConfiguration/v2.0">
	<system.web>
		<customErrors mode="off" />
	</system.web>
    <system.webServer>
        <staticContent>
            <mimeMap fileExtension=".json" mimeType="application/json" />
            <mimeMap fileExtension=".mp4" mimeType="video/mp4" />
        </staticContent>
        <httpProtocol>
            <customHeaders>
                <add name="Access-Control-Allow-Origin" value="*" />
            </customHeaders>
        </httpProtocol>
    </system.webServer>
</configuration>
